  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "tc": "tsc --noEmit --watch",
    "db:new": "dbmate new",
    "db:up": "dbmate up",
    "db:down": "dbmate down",
    "db:reset": "dbmate reset",
